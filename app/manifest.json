{
	"name": "ALauncher",
	"version": "1.0",
	"manifest_version": 2,
	"permissions": [
	"*://*/*","tabs","contextMenus"
	],
  "content_scripts": [
    {
      "matches": ["*://*.weibo.cn/*"],
      "css": [""],
      "js": [""]
    }
  ],
	"description": "An unofficial app launcher for chrome",
	"icons": {
		"16": "icon-weibo.png",
		"32": "icon-weibo.png",
		"48": "icon-weibo.png"
	},
	"background": {
		"scripts": ["dist/bg.html"]
	},
	"browser_action": {
		"default_icon": "icon-weibo.png",
		"default_title": "App Launcher",
		"default_popup": "dist/index.html"
	}
}
