{
  "manifest_version": 2,
  "name": "Unofficial Chrome App Launcher",
  "short_name": "UCA Launcher",
  "description": "",
  "version": "0.0.1",
  "minimum_chrome_version": "38",
  "permissions": [
    "webview",
  	"*://zaaack.github.io/uca-launcher",
  	"*://localhost:*/*"
